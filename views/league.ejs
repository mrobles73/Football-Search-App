<%- include('partials/header') -%>

<header class="p-3 bg-dark text-white">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        
            <img src="<%= imageURL %>" alt="<%= league %>" class="league-logo">
        

        <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
            <li><h1><%= league.toUpperCase() %></h1></li>
        </ul>

        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3">
          <input type="search" class="form-control form-control-dark my-search-input no-nums" placeholder="Search..." aria-label="Search">
        </form>

      </div>
    </div>
  </header>

  <div class="container mt-3">
  <table class="table table-dark table-striped table-bordered table-hover table-sortable">
    <thead>
      <tr>
        <th scope="col" class="col-1">Team</th>
        <th scope="col" class="col-11 clickable" id="team-name">Name</th>
      </tr>
    </thead>
    <tbody>
        <% data["response"].forEach((team)=>{ %>
      <tr class="team-clickable clickable" id="<%= team['team'].id %>" name="<%= team['team'].name %>">
        <td><img src="<%= team['team'].logo %>" alt="<%= team['team'].name %>" class="team-logo"></td>
        <td><%= team["team"].name %></td>
      </tr>
      <%}) %>
    </tbody>
  </table>
</div>

<%- include('partials/footer') -%>